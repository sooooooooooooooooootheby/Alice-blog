<home>
    <div class="homeHeader">
        <div class="homeHeaderBox">
            <div class="homeMain">
                <span><%= theme.homeIntroduce.one %></span>
                <p><%= theme.homeIntroduce.p1 %></p>
                <p class="tagline"><%= theme.homeIntroduce.p2 %> <img src="<%= theme.meme.SothebyTehe %>" alt=""></p>
                <div class="buttonBox">
                    <% for (item of theme.homeButton) { %>
                        <div class="button <%= item.buttonType %>"><a href="<%= item.url %>"><%= item.name %></a></div>
                    <% } %>
                </div>
            </div>
            <div class="homeLogo">
                <img src="<%= url_for('img/logo.webp') %>" alt="logo">
                <div></div>
            </div>
        </div>
    </div>
    <div class="homeContainer">
        <ul>
            <li>
                <img src="<%= url_for('img/logo.webp') %>" alt="logo">
                <span>我会什么呢</span>
                <p><del>我会玩重返未来:1999，我是1999高手！</del></p>
            </li>
            <% for (item of theme.homeContainer) { %>
                <li>
                    <svg class="icon" aria-hidden="true">
                        <use xlink:href="#icon-<%= item.svg %>"></use>
                    </svg>
                    <span><%= item.span %></span>
                    <p><%= item.p %></p>
                </li>
            <% } %>
        </ul>
    </div>
</home>
<%- partial('_partial/footer') %>